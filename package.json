  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "npm run rimraf -- dist",
    "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "json": "node tools/json.server.js",
    "postinstall": "typings install",
    "rimraf": "rimraf",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --watch --content-base src/ --port 7777",
    "server:prod": "http-server dist -p 7777 --cors",
    "start": "npm run server:dev",
    "prestart:prod": "npm run build",
    "start:prod": "npm run server:prod",
    "pretest": "npm run rimraf -- coverage",
    "test": "karma start config/karma.conf.js",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server"
  },
  "dependencies": {
    "@angular/common": "^2.0.2",
    "@angular/compiler": "^2.0.2",
    "@angular/core": "^2.0.2",
    "@angular/forms": "^2.0.2",
